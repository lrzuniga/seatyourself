

<%= form_for @restaurant do |f| %>

<%= f.label :name %> <br>
<%= f.text_field :name %> <br>

<%= f.label :address %> <br>
<%= f.text_field :address %> <br>

<%= f.label :description %> <br>
<%= f.text_area :description %> <br>

<%= f.label :seats %> <br>
<%= f.number_field :seats %> <br>

<%= f.submit %>

<% end %>